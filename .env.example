# PostgreSQL 测试数据库
DATABASE_URL="postgresql://hr_user:hr_password@localhost:5432/hr_agent_test"

# GitHub
GITHUB_TOKEN=
GITHUB_REPO_URL=
GITHUB_WEBHOOK_SECRET=

# Docker Coding Agent
DOCKER_CA_IMAGE=ghcr.io/eeymoo/open-hr-agent-ca:main
DOCKER_CA_PORT=4096
DOCKER_BASE_PORT=5000
DOCKER_NETWORK=hr-network
DOCKER_CA_SECRET=

# Coding Agent - SSH Keys for GitHub CLI
SSH_PRIVATE_KEY=
SSH_PUBLIC_KEY=

# Global Secret
# If HRA_SECRET is set, any other environment variable whose name ends with _SECRET
# and is left unset will default to value of HRA_SECRET.
HRA_SECRET=hr-agent-secret

# Server
PORT=3000
NODE_ENV=development
