# PostgreSQL 测试数据库
DATABASE_URL="postgresql://hr_user:hr_password@localhost:5432/hr_agent_test"

# GitHub
GITHUB_TOKEN=
GITHUB_REPO_URL=
GITHUB_WEBHOOK_SECRET=

# Docker Coding Agent
DOCKER_CA_IMAGE=ghcr.io/eeymoo/open-hr-agent-ca:main
DOCKER_CA_PORT=4096
DOCKER_BASE_PORT=5000
DOCKER_NETWORK=hr-network
DOCKER_CA_SECRET=

# Global Secret (used as default for all other secrets)
# If HRA_SECRET is set, it will be used as default for DOCKER_CA_SECRET and GITHUB_WEBHOOK_SECRET
# If individual secrets are not set, they will fall back to HRA_SECRET
HRA_SECRET=

# Server
PORT=3000
NODE_ENV=development
